<odoo>
    <record id="view_delivery_carrier_form_free_delivery_untaxed" model="ir.ui.view">
        <field name="name">view.delivery.carrier.form.free.delivery.untaxed</field>
        <field name="model">delivery.carrier</field>
        <field name="inherit_id" ref="delivery.view_delivery_carrier_form"/>
        <field name="arch" type="xml">
            <field name="amount" position="before">
                <field name="use_amount_untaxed" attrs="{'invisible': [('free_over', '=', False)]}"/>
            </field>
        </field>
    </record>
</odoo>
